        <script>
        function playersdata() {

        if(document.getElementById('playersdetailsid').children.length > 0){
         document.getElementById('playersdetailsid').removeChild(document.getElementById('playersdetailsid').children);
        }

        var maindiv = document.createElement("div");
        document.getElementById('playersdetailsid').appendChild(maindiv); 


        var teamlist_data = [{
                                "title": "5 PHOTOS",
                                "image_ground": "http://www.torontocitycricketclub.ca/siteimages/atnteam.jpg",
                                "image_country": "3 DAYS AGO",
                                "titlecode": "#TCCC at Feildian Grounds"
                        }, {
                                "title": "8 PHOTOS",
                                "image_ground": "http://www.torontocitycricketclub.ca/siteimages/icicibankcricketteam.jpg",
                                "image_country": "25/02/16",
                                "titlecode": "#TCCC at Tea Hill Park"
                        }, {
                                "title": "5 PHOTOS",
                                "image_ground": "http://www.torontocitycricketclub.ca/siteimages/kpmg.jpg",
                                "image_country": "18/02/16",
                                "titlecode": "#TCCC at The Candy Fields"
                        }, {
                                "title": "9 PHOTOS",
                                "image_ground": "http://www.torontocitycricketclub.ca/siteimages/thoundrbolts.jpg",
                                "image_country": "16/02/16",
                                "titlecode": "#TCCC at Maple Leaf Cricket Club"
                        }, {
                                "title": "5 PHOTOS",
                                "image_ground": "http://www.torontocitycricketclub.ca/siteimages/tlj.jpg",
                                "image_country": "15/02/16",
                                "titlecode": "#TCCC at Rideau Hall"
                        }];



        for (var i = 0; i < teamlist_data.length; i++) {
        var firstdiv = document.createElement("div");
        firstdiv.className="col-lg-4 col-md-4 col-sm-6 col-xs-6";
        maindiv.appendChild(firstdiv);

        var titlediv = document.createElement("div");
        titlediv.innerHTML="Team"+i;
        titlediv.className="titleforteams";
        firstdiv.appendChild(titlediv);


        var imagediv = document.createElement("img");
        imagediv.src=teamlist_data[i].image_ground;
        imagediv.className="img-responsive iton-platform-thumbs img-rounded";
        firstdiv.appendChild(imagediv); 


        var countdiv = document.createElement("div");
        countdiv.innerHTML= i+1;
        countdiv.className="countteamstyle";     
        firstdiv.appendChild(countdiv);

        }

        }

        </script>
        <script>
        function playersdata1234() {

        if(document.getElementById('palyersiddetails').children.length > 0){
         document.getElementById('palyersiddetails').removeChild(document.getElementById('palyersiddetails').children);
        }

        var maindiv = document.createElement("div");
        maindiv.className="playersheight";
        document.getElementById('palyersiddetails').appendChild(maindiv); 







for(var j=0;j<=2;j++){


        var teamname = document.createElement("div");
        teamname.innerHTML="Team Name";

        teamname.className="playerstextname";
        maindiv.appendChild(teamname); 



        for (var i = 0; i < 16; i++) {
        var firstdiv = document.createElement("div");
        firstdiv.className="teamplayerdiv";
        maindiv.appendChild(firstdiv);


        var imagediv = document.createElement("img");
        imagediv.src="http://www.torontocitycricketclub.ca/siteimages/tlj.jpg";
        imagediv.className="img-responsive iton-platform-thumbs img-rounded";
        firstdiv.appendChild(imagediv); 



        var namediv = document.createElement("div");
        namediv.innerHTML="Player - "+i;
        namediv.className="teamplayername";
        firstdiv.appendChild(namediv);



        }
}

        }
        </script>
